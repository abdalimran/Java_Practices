package imran.java.prog_176;

public class IPair 
{
	private int x,y;
	
	public IPair(int x, int y)
	{
		this.x=x;
		this.y=y;
	}
	
	public void setX(int x)
	{
		this.x=x;
	}
	public int getX()
	{
		return x;
	}
	public void setY(int y)
	{
		this.y=y;
	}
	public int getY()
	{
		return y;
	}
	
	@Override
	public String toString()
	{
		return "("+x+","+y+")";
	}
	
	public boolean equals(IPair a, IPair b)
	{
		
	}

}
